@model IEnumerable<DeliverEase.Models.ToDeliver>

@{
    ViewBag.Title = "Index";
}

<h1 class="lead">Pending Deliveries</h1>


<table class="table">
    <tr>
            
        <th>
            @Html.DisplayNameFor(model => model.IsDelivered)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsAccepted)
        </th>
        <th></th>
    </tr>
    
@foreach (var item in Model) {
    <tr>
        
        <td>
            @Html.DisplayFor(modelItem => item.IsDelivered)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsAccepted)
        </td>
        <td>
            @*@Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })*@
        </td>
    </tr>
}

</table>
